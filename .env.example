LEAPTABLE_JWT_SECRET=

# Bright Data
LEAPTABLE_BRIGHT_DATA_KEY=

# SERP API
SERP_API_KEY=

#API Base
NEXT_PUBLIC_API_BASE_URL=http://localhost:43001

